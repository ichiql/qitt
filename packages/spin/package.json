    "lint": "eslint ./src",
    "build": "vite build && tsc -p tsconfig.build.json",
    "prepublish": "pnpm build"
  },
  "keywords": [
    "qitt",
    "@qitt",
    "spin"
  ],
  "author": "Yamashita, Shinichi",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@qitt/config": "workspace:^",
    "@types/node": "^20.3.2",
    "jest": "^29.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  }
